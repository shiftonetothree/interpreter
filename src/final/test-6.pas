program Main;
    var a: integer;
    var b: integer;
    var c: integer;
begin { Main }
    a := 0;
    b := 0;
    c := 1;
    if a = 0 then
        a := a + 1;
    
    if b = 0 then
        b := b + 1;
    else
        b := b + 2;
    
    if c = 0 then
        c := c + 1;
    else
        c := c + 2;
end.  { Main }
